{
  "name": "@regolithco/common",
  "version": "1.4.1",
  "main": "dist/index.js",
  "module": "dist_esnext/index.js",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22 <24"
  },
  "scripts": {
    "build": "yarn build:gql; tsc -p tsconfig.json && tsc -p tsconfig.esnext.json",
    "watch": "concurrently \"tsc --watch -p tsconfig.json\" \"tsc --watch -p tsconfig.esnext.json\"",
    "watch:react": "tsc --watch -p tsconfig.esnext.json",
    "watch:server": "tsc --watch -p tsconfig.json",
    "build:gql": "graphql-codegen --config codegen.yml",
    "watch:gql": "graphql-codegen --watch src/*.gql --config codegen.yml",
    "lint": "tsc --noEmit && eslint src --fix",
    "test": "jest"
  },
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@parcel/watcher": "^2.5.1",
    "@types/eslint__eslintrc": "^3.3.0",
    "@types/eslint__js": "^9.14.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.16.5",
    "@types/numeral": "^2.0.5",
    "@types/webrtc": "^0.0.46",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-promise": "^7.2.1",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "jest": "^29.7.0",
    "papaparse": "^5.5.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "blob": "^0.1.0",
    "buffer": "^6.0.3",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "loglevel": "^1.9.2",
    "numeral": "^2.0.6"
  },
  "packageManager": "yarn@3.6.1"
}
