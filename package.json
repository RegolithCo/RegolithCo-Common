{
  "name": "@regolithco/common",
  "version": "1.0.56",
  "main": "dist/index.js",
  "module": "dist_esnext/index.js",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18 <19"
  },
  "scripts": {
    "build": "yarn build:gql; tsc -p tsconfig.json && tsc -p tsconfig.esnext.json",
    "watch": "concurrently \"tsc --watch -p tsconfig.json\" \"tsc --watch -p tsconfig.esnext.json\"",
    "watch:react": "tsc --watch -p tsconfig.esnext.json",
    "watch:server": "tsc --watch -p tsconfig.json",
    "build:gql": "graphql-codegen --config codegen.yml",
    "watch:gql": "graphql-codegen --watch src/*.gql --config codegen.yml",
    "lint": "tsc --noEmit && eslint src --fix",
    "test": "jest"
  },
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript": "^4.1.2",
    "@graphql-codegen/typescript-operations": "^4.4.0",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.9",
    "@types/numeral": "^2.0.5",
    "@types/webrtc": "^0.0.44",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "jest": "^29.7.0",
    "papaparse": "^5.5.1",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "uuid": "^10.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.12.11",
    "blob": "^0.1.0",
    "buffer": "^6.0.3",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "loglevel": "^1.9.2",
    "numeral": "^2.0.6"
  },
  "packageManager": "yarn@3.6.1"
}
